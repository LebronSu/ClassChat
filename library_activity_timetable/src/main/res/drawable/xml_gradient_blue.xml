<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 连框颜色值 -->
    <item>
        <shape>
            <solid android:color="#ffffff" />
        </shape>
    </item>
    <!-- 主体背景颜色值 -->
    <item>
        <shape>
            <corners android:radius="10dp"/>
            <gradient
                android:startColor="#ffffff"
                android:endColor="#ABDCFF"
                android:angle="45"
                />
        </shape>
    </item>
</layer-list>
